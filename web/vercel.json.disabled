{
  "rewrites": [
    {
      "source": "/maintenance",
      "destination": "/maintenance.html"
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "missing"
        },
        {
          "type": "header",
          "key": "accept",
          "value": ".*text/html.*"
        }
      ],
      "destination": "/maintenance"
    }
  ]
}
